{
    "name": "owncloud/files_external_dropbox",
    "config": {
        "platform": {
            "php": "7.3"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Hemant-Mann/dropbox-php-sdk"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Hemant-Mann/flysystem-dropbox"
        }
    ],
    "require": {
        "kunalvarma05/dropbox-php-sdk": "dev-refresh_tokens_support",
        "hemant-mann/flysystem-dropbox": "dev-guzzle5"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4"
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false
        }
    }
}
